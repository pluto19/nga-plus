{
  "manifest_version": 3,
  "name": "NGA-Plus",
  "version": "1.0",
  "description": "在新标签页打开NGA论坛的帖子链接",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://bbs.nga.cn/*", "*://*.nga.cn/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": []
}